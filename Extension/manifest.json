{
    "manifest_version": 3,
    "name": "Internshala Application Helper",
    "version": "1.0",
    "description": "Automate your internship applications on Internshala.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://internshala.com/internships/"],
        "js": ["content.js"]
      }
    ]
  }
  